{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "sourceMap": false,
        "outDir": "build",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "emitDecoratorMetadata": false,
        "experimentalDecorators": false,
    },
    "exclude": [
        "node_modules", "src/web"
    ],
    "include": [
        "src/**/*"
, "main.ts", "main.ts"]
}