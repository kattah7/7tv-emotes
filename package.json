    "morgan": "^1.10.0",
    "node-fetch": "2",
    "nodemon": "^2.0.19",
    "sirv-cli": "^2.0.0",
    "svelte-routing": "^1.6.0",
    "typescript": "^4.8.3",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.0.0",
