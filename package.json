    "lint:fix": "eslint './**/*.{js,ts}' --fix",
    "format": "prettier --check './**/*.{js,ts}'",
    "test": "jest -c .jestrc.js --verbose --passWithNoTests",
    "web:dev": "rollup -c -w",
    "web:start": "sirv src/web/public --no-clear -s"
